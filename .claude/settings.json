      "Bash(dd:*)",
      "Bash(mkfs:*)",
      "Bash(fdisk:*)",
      "Bash(reboot:*)",
      "Bash(shutdown:*)"
    ],
    "ask": [
      "Bash(sudo:*)"
    ]
  }
